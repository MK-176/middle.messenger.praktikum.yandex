{
  "extends": [
    "@parcel/config-default"
  ],
  "transformers": {
    "*.{js,mjs,jsx,cjs,ts,tsx}": [
  	  "@parcel/transformer-js"
  	],
    "*.scss": [
      "@parcel/transformer-sass"
    ],
    "*.hbs": [
      "@anikin/parcel-transformer-handlebars-precompile"
    ]
  },
  "validators": {
    "*.{ts,tsx}": ["@parcel/validator-typescript"]
  }
}
